{
  "name": "fullstack-takehome-ts-react-node",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev:frontend": "vite",
    "dev:backend": "nodemon --watch './**/*.ts' --exec ts-node-esm ./server/server.ts",
    "dev": "concurrently 'npm:dev:frontend' 'npm:dev:backend' ",
